# Focused window border
focus #202020

# Switcher window border
menubc #202020

# Switcher background 
menubg #2c2c2c

# Switcher text
menufg #aaaaaa

# Switcher highlight background
menuhlbg #5c5c5c

# Switcher highlight text
menuhlfg #bbbbbb

# Launcher colours
launcher dmenu_run -nb "#2C2C2C" -nf "#AAAAAA" -sb "#5C5C5C" -sf "#BBBBBB" 

# Auto-start xscreensaver
#auto xscreensaver -nosplash

auto xsetroot -solid Grey8

# Focus mode
focusmode click

